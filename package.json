{
	"name": "apollo",
	"version": "1.0.0",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"files": [
		"dist",
		"src"
	],
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"scripts": {
		"lint": "biome check --write",
		"build": "tsup && knip && lint",
		"test": "vitest run",
		"knip": "knip",
		"build-rolldown": "rolldown -c rolldown.config.ts"
	},
	"keywords": [
		"fetch",
		"fetch-api",
		"fetch wrapper",
		"http",
		"http-client",
		"api",
		"got",
		"axios",
		"ky",
		"request",
		"apollo",
		"graphql"
	],
	"author": "",
	"license": "ISC",
	"description": "Apollo is a library for building Apollo Client",
	"devDependencies": {
		"@biomejs/biome": "2.0.0",
		"@standard-schema/spec": "^1.0.0",
		"@types/node": "^24.0.1",
		"knip": "^5.61.2",
		"msw": "^2.0.0",
		"rolldown": "1.0.0-beta.20",
		"rolldown-plugin-dts": "^0.13.12",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3",
		"vitest": "^3.2.3"
	},
	"engines": {
		"node": ">=20"
	},
	"publishConfig": {
		"access": "public"
	}
}